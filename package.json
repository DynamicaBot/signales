{
  "name": "@dynamicabot/signales",
  "version": "2.0.15",
  "description": "Highly configurable logging utility",
  "license": "MIT",
  "repository": "dynamicabot/signales",
  "scripts": {
    "test": "tsd",
    "build": "tsup",
    "release": "semantic-release"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "license.md"
  ],
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/main.js",
      "import": "./dist/main.mjs",
      "types": "./dist/main.d.ts"
    }
  },
  "keywords": [
    "log",
    "cli",
    "logger",
    "logging",
    "hackable",
    "colorful",
    "console"
  ],
  "tsd": {
    "directory": "test-d"
  },
  "dependencies": {
    "chalk": "^5.1.2",
    "figures": "^6.0.0",
    "strip-ansi": "^7.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.0.0",
    "semantic-release": "^22.0.7",
    "signale": "^1.4.0",
    "tsd": "^0.29.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Klaus Sinani",
    "email": "klaussinani@gmail.com",
    "url": "https://klaussinani.github.io"
  },
  "maintainers": [
    {
      "name": "Sebastian Pietschner",
      "email": "sebastian@sebasptsch.dev",
      "url": "https://www.sebasptsch.dev"
    }
  ],
  "release": {
    "branches": [
      "master"
    ],
    "repositoryUrl": "https://github.com/dynamicabot/signales",
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
